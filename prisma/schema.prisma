generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  idx       Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  age       Int?
  visits    Int?
  progress  Int?
  status    String?
  createdAt DateTime @default(now())
}

